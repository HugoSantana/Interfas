using System;
public class Sesion
{
        public  void Main()
    {
                string usuario;
                int login, contraseña;
        
                do
                {
                        Console.WriteLine("Escriba su Usuario");
                        usuario = Console.ReadLine();
                        Console.WriteLine("Escriba su Contraseña");
            login= Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("vuelva a teclear su contraseña");
                        contraseña = Convert.ToInt32(Console.ReadLine());
                        if (login != contraseña)
                                Console.WriteLine("Contraseña Incoreecta..!!!");
                }
                while (login != contraseña);
                Console.WriteLine("contraseña correcta");
                {
          Console.ReadLine();
                }
        }
}
